{
  "name": "with-scss",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "dev": "concurrently -kill-others -p none \"npm run start\" \"npm run watch\"",
    "start": "live-server . --port=5500",
    "watch": "sass --watch --no-source-map --style=compressed src/scss:src/css"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.4.0",
    "live-server": "^1.2.2",
    "sass": "^1.54.9"
  }
}
